<% provide(:title, 'Sign in') %>
<div class="signin">
  <h1>Sign in</h1>
  
  <div class="#">
    <div class="form_wrapper">
      <%= form_with(model: @user, local: true) do |f| %>
        <%= render 'shared/error_messages' %>
  
        <%= f.label :email %>
        <%= f.email_field :email, class: "form_parts" %>
  
        <%= f.label :password %>
        <%= f.password_field :password, class: "form_parts" %>
  
  
        <%= f.submit "Log In", class: "form_parts create_btn" %>
      <% end %>
    </div>
  </div>
</div>

